<tr>
    <th>
        <%= account_id = account.account_id
          link_to account.account_id, account %>
    </th>
    <td>
        <%= account.name %>
    </td>
    <td>
        <%= account.balance %>
    </td>
    <td>
        <%= account.currency %>
    </td>
    <td>
        <%= account.nature %>
    </td>
    <td>
        <%= account.transactions_count %>
    </td>
</tr>